}

@article{Asylumarchitecture2016,
  title = {Asylum Architecture},
  copyright = {Creative Commons Attribution-ShareAlike License},
  abstract = {Asylum Architecture in the United States, including the architecture of psychiatric hospitals, affected the changing methods of treating the mentally ill in the nineteenth century: the architecture was considered part of the cure. Doctors believed that ninety percent of insanity cases were curable, but only if treated outside the home, in large-scale buildings. Nineteenth-century psychiatrists considered the architecture of asylums, especially their planning, to be one of the most powerful tools for the treatment of the insane, targeting social as well as biological factors to facilitate the treatment of mental illnesses. The construction and usage of these quasi-public buildings served to legitimize developing ideas in psychiatry. About 300 psychiatric hospitals, known at the time as insane asylums or colloquially as ``loony bins'' or ``nuthouses,'' were constructed in the United States before 1900. Asylum architecture is notable for the way similar floor plans were built in a wide range of architectural styles.},
  language = {en},
  journal = {Wikipedia},
  month = jul,
  year = {2016},
  file = {C:\\Users\\alec\\Zotero\\storage\\3XQ7GPVD\\index.html},
