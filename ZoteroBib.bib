  publisher = {{Birkh{\"a}user Basel}},
  author = {Hanemaaijer, Roeland and van Lent, Natascha and Sorsa, Timo and Salo, Tuula and Yrj{\H O} and Konttinen, T. and Lindeman, Jan},
  editor = {Nelson, Dr Mark and Hillen, Prof Dr Wolfgang and Greenwald, Prof Dr Robert A.},
  year = {2001},
  keywords = {Biochemistry; general,Medical Biochemistry,Pharmacology/Toxicology},
  pages = {267--281},
  file = {C:\\Users\\alec\\Zotero\\storage\\KHNW47KR\\10.html},
